package main

import (
    "fmt"
    "os"
    "github.com/likexian/whois"
    "github.com/likexian/whois-parser"
)

func main() {
    result, err := whois.Whois("google.com")
    if err != nil {
        fmt.Println(err)
        os.Exit(1)
    }
    
    parsedData, err := whoisparser.Parse(result)
    if err != nil {
        fmt.Println(err)
        os.Exit(1)
    }

    fmt.Println(parsedData.Domain.ExpirationDate)
}
